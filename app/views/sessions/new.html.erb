<% provide(:title, 'Sign in') %>
<div class="row">
	<div class="span6 offset3">
		<h2>Sign in</h2>
		<%= form_for :sessions, url:sessions_path do |f| %>
		<%= f.label :email%>
		<%= f.text_field :email %>
		<%= f.label :password %>
		<%= f.password_field :password %>
		<%= f.submit "Submit", class:"btn btn-primary" %>
		<% end %>
	</div>
</div>

